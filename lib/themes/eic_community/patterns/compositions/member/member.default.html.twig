{% extends "@theme/patterns/layout/base.html.twig" %}

{% block breadcrumbs %}{% endblock %}

{% block page_header %}
  {% if overview_header.image is not empty %}
    {% include "@theme/patterns/compositions/overview-header.html.twig" with overview_header|default({}) only %}
    {% include "@theme/patterns/compositions/breadcrumb.html.twig" with breadcrumb|default({}) only %}
  {% else %}
    {% include "@theme/patterns/compositions/breadcrumb.html.twig" with breadcrumb|default({}) only %}
    {% include "@theme/patterns/compositions/overview-header.html.twig" with overview_header|default({}) only %}
  {% endif %}
  {% if subnavigation %}
    {% include "@theme/patterns/compositions/subnavigation.html.twig" with subnavigation|default({})|merge({
      icon_file_path: icon_file_path
    }) only %}
  {% endif %}
{% endblock %}

{% block content %}
    {#
      CONTENT
    #}
  {% include "@theme/patterns/compositions/featured-content-sections.html.twig" with {
    as_tabs: true,
    items: [
      flagged_interest|default({}),
      flagged_groups|default({}),
      flagged_event|default({}),
      flagged_comments|default({}),
    ]
  } only %}
{% endblock %}

{% block sidebar %}
{% endblock %}
