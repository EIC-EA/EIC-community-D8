{
  "name": "eic-community",
  "version": "0.1.0",
  "description": "The EIC community Drupal 8 Platform",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all --parallel compile:* -ln",
    "compile:copy": "ecl-builder copy",
    "compile:scripts": "ecl-builder scripts",
    "compile:styles": "ecl-builder styles",
    "compile:svg": "node ./scripts/svg-sprites.js",
    "watch": "npm-run-all --parallel watch:* -ln",
    "watch:scripts": "chokidar \"./js/**/*.js\" \"./react/**/*.js\" -i \"/node_modules/\" -i \"./react/svg/svg.js\" -c \"npm-run-all compile:scripts react-production\"",
    "watch:styles": "chokidar \"./sass/**/*.scss\" -i \"/node_modules/\" -c \"npm run compile:styles\"",
    "watch:svg": "chokidar \"./images/**/*.svg\" -i \"/node_modules/\" -c \"npm-run-all compile:svg react-production\"",
    "prestorybook": "npm-run-all compile:*",
    "storybook": "start-storybook -s ./dist -p 9000",
    "pregenerate-storybook": "[ -d './storybook-static' ] && rm -r ./storybook-static || exit 0",
    "generate-storybook": "build-storybook -o ./storybook-static",
    "postgenerate-storybook": "[ -d './dist' ] && cp -r ./dist/* ./storybook-static || exit 0",
    "predev": "[ -d './node_modules/.cache/storybook' ] && rm -r ./node_modules/.cache/storybook || exit 0",
    "dev": "npm-run-all --parallel storybook watch",
    "react-development": "mix",
    "react-watch": "mix watch",
    "react-watch-poll": "mix watch -- --watch-options-poll=1000",
    "react-hot": "mix watch --hot",
    "react-production": "mix --production",
    "mail-watch": "mjml -w mails/src/* -o mails/mail-dist/",
    "mail-build": "mjml mails/src/* -o mails/mail-dist/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@ecl-twig/ec-components": "^2.35.2",
    "@ecl/builder": "^2.35.0",
    "@ecl/ec-base": "^2.35.0",
    "@ecl/ec-component-form-help-block": "^2.35.0",
    "@ecl/ec-preset-editor": "^2.35.0",
    "@ecl/ec-preset-legacy-website": "^2.35.0",
    "@ecl/ec-theme-default": "^2.35.0",
    "@ecl/ec-utility-background": "^2.35.0",
    "@ecl/ec-utility-border": "^2.35.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-viewport": "^6.3.6",
    "@storybook/builder-webpack5": "^6.3.6",
    "@storybook/html": "^6.3.6",
    "@storybook/manager-webpack5": "^6.3.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-drupal-behaviors": "^0.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.0",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.4.0",
    "flag-icons": "^2.0.0",
    "glob": "^7.1.6",
    "imagemin": "^7.0.1",
    "imagemin-svgo": "^8.0.0",
    "laravel-mix": "^6.0.19",
    "laravel-mix-svg": "^0.4.1",
    "mjml": "^4.10.3",
    "mkdirp": "^1.0.4",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.2.1",
    "svg4everybody": "^2.1.9",
    "svgstore": "^3.0.0-2",
    "twing": "2.3.7",
    "twing-loader": "2.0.2",
    "unicode-emoji-picker": "^1.1.1",
    "webpack": "^5.9.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.13.13",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "axios": "^0.21.1",
    "disqus-react": "^1.1.2",
    "postcss": "^8.3.6",
    "react": "^16.9.0",
    "react-dom": "^16.14.0",
    "react-notifications-component": "^3.1.0",
    "react-test-renderer": "^16.9.0",
    "reactjs-popup": "^2.0.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  }
}
