{
  "name": "eic-community",
  "version": "0.1.0",
  "description": "The EIC community Drupal 8 Platform",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all --parallel build:* -ln",
    "build:copy": "ecl-builder copy",
    "build:styles": "ecl-builder styles",
    "prestorybook": "npm run build",
    "storybook": "start-storybook -s ./dist -p 6006",
    "watch": "npm-run-all --parallel watch:* -ln",
    "watch:styles": "chokidar \"./sass/**/*.scss\" -i \"/node_modules/\" -c \"npm run build:styles\"",
    "watch:scripts": "chokidar \"./js/**/*.js\" -i \"/node_modules/\" -c \"npm run build:copy\"",
    "dev": "npm-run-all --parallel storybook watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@ecl-twig/ec-components": "^2.35.1",
    "@ecl/builder": "^2.35.0",
    "@ecl/ec-base": "^2.35.0",
    "@ecl/ec-component-form-help-block": "^2.35.0",
    "@ecl/ec-preset-editor": "^2.35.0",
    "@ecl/ec-preset-legacy-website": "^2.35.0",
    "@ecl/ec-theme-default": "^2.35.0",
    "@ecl/ec-utility-background": "^2.35.0",
    "@ecl/ec-utility-border": "^2.35.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/html": "^6.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-drupal-behaviors": "^0.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chokidar-cli": "^2.1.0",
    "chokidar": "^3.5.0",
    "glob": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "svg4everybody": "^2.1.9",
    "twing": "5.0.2",
    "twing-loader": "3.0.1"
  }
}
