{
  "name": "eic-community",
  "version": "0.1.0",
  "description": "The EIC community Drupal 8 Platform",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all --parallel compile:* -ln",
    "compile:copy": "ecl-builder copy",
    "compile:scripts": "ecl-builder scripts",
    "compile:styles": "ecl-builder styles",
    "compile:svg": "node ./scripts/svg-sprites.js",
    "watch": "npm-run-all --parallel watch:* -ln",
    "watch:scripts": "chokidar \"./js/**/*.js\" -i \"/node_modules/\" -c \"npm run build:scripts\"",
    "watch:styles": "chokidar \"./sass/**/*.scss\" -i \"/node_modules/\" -c \"npm run build:styles\"",
    "watch:svg": "chokidar \"./images/**/*.svg\" -i \"/node_modules/\" -c \"npm run build:svg\"",
    "prestorybook": "npm-run-all compile:*",
    "storybook": "start-storybook -s ./dist -p 8080",
    "pregenerate-storybook": "[ -d './storybook-static' ] && rm -r ./storybook-static || exit 0",
    "generate-storybook": "build-storybook -o ./storybook-static",
    "postgenerate-storybook": "[ -d './dist' ] && cp -r ./dist/* ./storybook-static || exit 0",
    "predev": "[ -d './node_modules/.cache/storybook' ] && rm -r ./node_modules/.cache/storybook || exit 0",
    "dev": "npm-run-all --parallel storybook watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@ecl-twig/ec-components": "^2.35.1",
    "@ecl/builder": "^2.35.0",
    "@ecl/ec-base": "^2.35.0",
    "@ecl/ec-component-form-help-block": "^2.35.0",
    "@ecl/ec-preset-editor": "^2.35.0",
    "@ecl/ec-preset-legacy-website": "^2.35.0",
    "@ecl/ec-theme-default": "^2.35.0",
    "@ecl/ec-utility-background": "^2.35.0",
    "@ecl/ec-utility-border": "^2.35.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/html": "^6.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-drupal-behaviors": "^0.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.0",
    "chokidar-cli": "^2.1.0",
    "chokidar": "^3.5.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint": "^7.17.0",
    "glob": "^7.1.6",
    "imagemin-svgo": "^8.0.0",
    "imagemin": "^7.0.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "svg4everybody": "^2.1.9",
    "svgstore": "^3.0.0-2",
    "twing-loader": "2.0.2",
    "twing": "2.3.7"
  }
}
