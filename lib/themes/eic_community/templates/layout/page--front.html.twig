{#
/**
 * @file
 * Theme override to display a single page.
 *
 * @see ./themes/custom/eic_community/patterns/pages/homepage.private.html.twig
 */
#}
{% set _wrap_content = true %}

{% extends "@theme/patterns/layout/base.html.twig" %}

{% block content %}
  {{ front.top }}

  {% embed "@theme/patterns/compositions/layout-block.html.twig" with {
    extra_classes: 'ecl-section-wrapper--is-white',
  } %}
    {% block content %}
      {{ front.middle_left }}
    {% endblock %}
    {% block aside %}
      {{ front.middle_right }}
    {% endblock %}
  {% endembed %}

  {{ front.bottom }}
{% endblock %}
