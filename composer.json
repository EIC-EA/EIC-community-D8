{
    "name": "eic-ea/eic-community-d8",
    "description": "The EIC community Drupal 8 Platform.",
    "type": "project",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "composer/composer": "1.10.23 as 2.0.13",
        "composer/installers": "^1.9",
        "composer/semver": "1.7.2 as 3.2.4",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/block_content_permissions": "^1.10",
        "drupal/block_field": "^1.0@RC",
        "drupal/components": "^2.2",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^1.7",
        "drupal/context": "^4.0@beta",
        "drupal/core": "^9.1",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/date_popup": "^1.1",
        "drupal/default_content": "^2.0@alpha",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/extra_field": "^2.0",
        "drupal/facets": "^1.7",
        "drupal/field_group": "^3.2",
        "drupal/flag": "^4.0@beta",
        "drupal/flag_search_api": "1.x-dev#3ffb747090f79e32943ebcc6a1b5fa01976e29c2",
        "drupal/fragments": "^2.0",
        "drupal/ginvite": "^2.0.0",
        "drupal/grequest": "^1.0",
        "drupal/group": "^1.3",
        "drupal/group_content_menu": "^1.0",
        "drupal/group_flex": "^1.0@beta",
        "drupal/group_permissions": "^1.0@alpha",
        "drupal/media_entity_download": "^2.1",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/message": "^1.2",
        "drupal/message_notify": "^1.2",
        "drupal/message_subscribe": "^1.1",
        "drupal/metatag": "^1.15",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/node_read_time": "^1.3",
        "drupal/openapi": "^2.0@RC",
        "drupal/openapi_rest": "^2.0@RC",
        "drupal/paragraph_view_mode": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/profile": "^1.2",
        "drupal/publication_date": "^2.0@beta",
        "drupal/realname": "^2.0-@beta",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/restui": "^1.19",
        "drupal/route_condition": "^2.0",
        "drupal/scheduler": "^1.3",
        "drupal/scheduler_content_moderation_integration": "^1.3",
        "drupal/search_api_attachments": "^1.0@beta",
        "drupal/search_api_solr": "^4.1",
        "drupal/search_api_sorts": "^1.0",
        "drupal/search_api_spellcheck": "^3.0@beta",
        "drupal/smart_trim": "^1.3",
        "drupal/smtp": "^1.0",
        "drupal/social_link_field": "^1.1@alpha",
        "drupal/subpathauto": "1.1.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/typed_link": "^2.0",
        "drupal/ui_patterns": "^1.2",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_operations": "^3.10",
        "drupal/views_data_export": "^1.0",
        "drupal/views_data_export_phpspreadsheet": "^1.2",
        "drush/drush": "^10.3",
        "ethosce/group_purl": "2.x-dev",
        "lramarojaona/purl": "^1.0@alpha",
        "openeuropa/ecl-twig-loader": "^2.12",
        "openeuropa/oe_media": "^1.13",
        "openeuropa/oe_time_caching": "^1.1",
        "openeuropa/oe_webtools": "^1.13",
        "softcreatr/jsonpath": "^0.7.5",
        "twig/twig": "2.14.3 as 1.44.2",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9.1",
        "drupal/data_fixtures": "^1.7",
        "drupal/devel": "^4.1",
        "drupal/drupal-extension": "~4.0",
        "ec-europa/qa-automation": "^3.0|^4.0",
        "ec-europa/toolkit": "^4.1",
        "mglaman/phpstan-drupal": "^0.12.13",
        "openeuropa/code-review": "^1.0",
        "openeuropa/task-runner": "^0.6",
        "phpspec/prophecy-phpunit": "^2",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "extra": {
        "artifacts": {
            "openeuropa/oe_theme": {
                "dist": {
                    "url": "https://github.com/4hardfire/oe_theme/archive/Issue-737_Fix_drupal_9_compatibilities.zip",
                    "type": "zip"
                }
            }
        },
        "composer-exit-on-patch-failure": true,
        "patches-file": "composer.patches.json",
        "enable-patching": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Site\\Tests\\": "./tests/"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3.21"
        }
    }
}
